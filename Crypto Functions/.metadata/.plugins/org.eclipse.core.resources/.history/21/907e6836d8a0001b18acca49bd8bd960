package Crypto;

import java.util.Scanner;

public class Main {

	public static RSAMode obj = new RSAMode();
	public static void main(String[] args) throws Exception {
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter 1 for AES: ");
		int opt = sc.nextInt();
		if(opt == 1) {
			AESMode.AES();
		}
		else if(opt == 2)
			obj.startRSA(1024);
		
		
	}

}